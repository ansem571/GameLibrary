<GameManager>
	<Properties>
		<Player>
			<Properties>
				<Name>string</Name>
				<Location>
					<Properties>
						<Point>
							<X>int</X>
							<Y>int</Y>
						</Point>
						<RegionId>string</RegionId>
					</Properties>
					<Functionality/>
				</Location>
				<RespawnLocation>ILocation
				</RespawnLocation>
				<Stats>
					<Properties>
						<Level>int</Level>
						<CurrentHealth>int</CurrentHealth>
						<MaxHealth>int</MaxHealth>
						<CurrentMana>int</CurrentMana>
						<MaxMana>int</MaxMana>
						<P.Atk>int</P.Atk>
						<P.Def>int</P.Def>
						<M.Atk>int</M.Atk>
						<M.Def>int</M.Def>
						<Speed>int</Speed>
						<CurrentExp>int</CurrentExp>
						<MaxExp>int</MaxExp>
						<Deaths>int</Deaths>
						<Gold>int</Gold>
					</Properties>
					<Functionality>
						<LevelUp>
							<Type>void</Type>
						</LevelUp>
						<DisplayStats>
							<Type>void</Type>
						</DisplayStats>
					</Functionality>
				</Stats>
				<MonsterCollection>Dictionary
					<Key>string</Key>
					<Value>int</Value>
				</MonsterCollection>
				<CharacterMovement>
					<Properties>
						<MaxWidth>int</MaxWidth>
						<MaxHeight>int</MaxHeight>
					</Properties>
					<Functionality>
						<ResetPlayerToRespawn>
							<Type>void</Type>
							<Params>ILocation</Params>
						</ResetPlayerToRespawn>
						<PerformMovement>
							<Type>void</Type>
							<Params>
								<Point/>
								<IMovementInput/>
							</Params>
						</PerformMovement>
					</Functionality>
				</CharacterMovement>
			</Properties>
			<Functionality>
				<Attack>
					<Type>void</Type>
					<Params>
						ICharacter opp, bool(useMagic)
					</Params>
				</Attack>
				<LoadFromFile>
					<Type>void</Type>
					<Params/>
				</LoadFromFile>
			</Functionality>
		</Player>
		<Map>
			<Properties>
				<MapGrid>Dictionary
					<Key>
						<Point/>
					</Key>
					<Value>
						<Tile>
							<Properties>
								<Location>ILocation</Location>
								<RegionId>string</RegionId>
								<Visited>bool</Visited>
							</Properties>
							<Functionality>
								<EnteredTile>
									<Type>void</Type>
									<Params>
										Player, object[] params <!-- These change based on the tile type-->
									</Params>
								</EnteredTile>
							</Functionality>
						</Tile>
					</Value>
				</MapGrid>
			</Properties>
			<Functionality>
				<OpenMap>
					<Type>void</Type>
					<Params>
						Action, Player.Location.Point
					</Params>
				</OpenMap>
				<RefreshMap>
					<Type>void</Type>
					<Params>
						Action, Player.Location.Point
					</Params>
				</RefreshMap>
				<CloseMap>
					<Type>void</Type>
					<Params>
						Action, Player.Location.Point
					</Params>
				</CloseMap>
			</Functionality>
		</Map>
	</Properties>
	<Functionality>
		<Play>
			<Type>void</Type>
		</Play>
		<Save>
			<Type>void</Type>
		</Save>
		<Load>
			<Type>void</Type>
		</Load>
		<Rest>
			<Type>void</Type>
		</Rest>
		<Quit>
			<Type>void</Type>
		</Quit>
	</Functionality>
	<PF>
		<OpenMap>
			<Type>void</Type>
		</OpenMap>
	</PF>
</GameManager>